<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#1a1b26" />
  <text x="400" y="50" font-family="Arial" font-size="24" fill="white" text-anchor="middle">MySQL - Create table</text>
  
  <!-- Panel principal -->
  <rect x="100" y="100" width="600" height="400" rx="10" fill="#282a36" stroke="#6272a4" stroke-width="2" />
  
  <!-- Título de la sección -->
  <text x="400" y="130" font-family="Arial" font-size="20" fill="white" text-anchor="middle">Create table</text>
  
  <!-- Campo de nombre de tabla -->
  <text x="150" y="170" font-family="Arial" font-size="14" fill="#8be9fd" text-anchor="start">table name</text>
  <rect x="150" y="180" width="500" height="30" rx="5" fill="#44475a" stroke="#6272a4" stroke-width="1" />
  <text x="160" y="200" font-family="Arial" font-size="14" fill="#f8f8f2" text-anchor="start">my-table</text>
  
  <!-- Encabezados de columnas -->
  <text x="150" y="240" font-family="Arial" font-size="14" fill="#8be9fd" text-anchor="start">column name</text>
  <text x="300" y="240" font-family="Arial" font-size="14" fill="#8be9fd" text-anchor="start">type</text>
  <text x="450" y="240" font-family="Arial" font-size="14" fill="#8be9fd" text-anchor="start">default</text>
  <text x="600" y="240" font-family="Arial" font-size="14" fill="#8be9fd" text-anchor="start">constraints</text>
  
  <!-- Primera fila de columna -->
  <rect x="150" y="250" width="130" height="30" rx="5" fill="#44475a" stroke="#6272a4" stroke-width="1" />
  <text x="160" y="270" font-family="Arial" font-size="14" fill="#f8f8f2" text-anchor="start">id</text>
  
  <rect x="300" y="250" width="130" height="30" rx="5" fill="#44475a" stroke="#6272a4" stroke-width="1" />
  <text x="310" y="270" font-family="Arial" font-size="14" fill="#f8f8f2" text-anchor="start">serial</text>
  
  <rect x="450" y="250" width="130" height="30" rx="5" fill="#44475a" stroke="#6272a4" stroke-width="1" />
  <text x="460" y="270" font-family="Arial" font-size="14" fill="#f8f8f2" text-anchor="start">no default</text>
  
  <rect x="600" y="250" width="130" height="30" rx="5" fill="#44475a" stroke="#6272a4" stroke-width="1" />
  <text x="610" y="270" font-family="Arial" font-size="14" fill="#f8f8f2" text-anchor="start">Primary Key</text>
  
  <!-- Botón de añadir columna -->
  <rect x="150" y="300" width="120" height="35" rx="5" fill="#3b82f6" stroke="#6272a4" stroke-width="1" />
  <text x="210" y="323" font-family="Arial" font-size="14" fill="white" text-anchor="middle">Add column</text>
  
  <!-- Botones de acción -->
  <rect x="500" y="400" width="100" height="35" rx="5" fill="#44475a" stroke="#6272a4" stroke-width="1" />
  <text x="550" y="423" font-family="Arial" font-size="14" fill="white" text-anchor="middle">Cancel</text>
  
  <rect x="620" y="400" width="100" height="35" rx="5" fill="#8b5cf6" stroke="#6272a4" stroke-width="1" />
  <text x="670" y="423" font-family="Arial" font-size="14" fill="white" text-anchor="middle">Create</text>
</svg>